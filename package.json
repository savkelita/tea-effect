{
  "name": "tea-effect",
  "version": "0.1.1",
  "description": "The Elm Architecture for TypeScript with Effect",
  "keywords": [
    "elm",
    "tea",
    "the-elm-architecture",
    "effect",
    "effect-ts",
    "functional-programming",
    "react",
    "typescript"
  ],
  "author": "Marko Savic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/savkelita/tea-effect"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./Cmd": {
      "types": "./dist/Cmd.d.ts",
      "import": "./dist/Cmd.mjs",
      "require": "./dist/Cmd.js"
    },
    "./Sub": {
      "types": "./dist/Sub.d.ts",
      "import": "./dist/Sub.mjs",
      "require": "./dist/Sub.js"
    },
    "./Task": {
      "types": "./dist/Task.d.ts",
      "import": "./dist/Task.mjs",
      "require": "./dist/Task.js"
    },
    "./Platform": {
      "types": "./dist/Platform.d.ts",
      "import": "./dist/Platform.mjs",
      "require": "./dist/Platform.js"
    },
    "./Html": {
      "types": "./dist/Html.d.ts",
      "import": "./dist/Html.mjs",
      "require": "./dist/Html.js"
    },
    "./React": {
      "types": "./dist/React.d.ts",
      "import": "./dist/React.mjs",
      "require": "./dist/React.js"
    },
    "./Navigation": {
      "types": "./dist/Navigation.d.ts",
      "import": "./dist/Navigation.mjs",
      "require": "./dist/Navigation.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest",
    "example:counter": "npx tsx examples/test-counter.ts",
    "example:userlist": "npx tsx examples/test-runtime.ts",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "effect": "^3.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.0",
    "effect": "^3.11.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
